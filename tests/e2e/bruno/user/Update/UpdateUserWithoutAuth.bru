meta {
  name: UpdateUserWithoutAuth
  type: http
  seq: 7
}

patch {
  url: {{baseURL}}/user/{{userToUpdateId}}
  body: json
  auth: none
}

headers {
  Accept-Language: es-ES
}

body:json {
  {
    "name": "Updated Without Auth"
  }
}

script:post-response {
  if (!res) {
    console.error("Response is undefined");
    return;
  }

  test("Status code is 401 (Unauthorized)", function () {
      expect(res.getStatus()).to.equal(401);
  });
}

settings {
  encodeUrl: true
  timeout: 0
}
